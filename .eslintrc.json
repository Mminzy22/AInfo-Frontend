{
  "env": {
    "browser": true,
    "es6": true,
    "es2021": true
  },
  "extends": "eslint:recommended", 
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": "off"
  },
  "globals": {
    "Kakao": "readonly",
    "google": "readonly",
    "axios": "readonly",
    "marked": "readonly"
  }
}
